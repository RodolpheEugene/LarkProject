echo "A picture of a clown appear on the wall, the killer clown is known for being very malicious"
read -p "You still wish to continue? [y/n]: " clown
case $clown in
Y|y) echo "You have made a very bad decision"
	sleep 2
	cd Area~
	$SHELL
	;;
N|n)  echo "You decided to stay in this room"
	
	;;
*) echo "Please pick [y/n]!"
	;;
esac
